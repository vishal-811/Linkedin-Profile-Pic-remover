{
    "manifest_version": 3,
    "name": "Linkedin Profile Picture Remover",
    "version": "1.0.0",
    "description": "Replaces all profile pictures on LinkedIn home/feed page with a single image.",
    "permissions": ["activeTab"],
    "host_permissions": ["https://www.linkedin.com/feed/"],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/feed/"],
        "js": ["content.js"]
      }
    ]
  }
  